{
    "name": "electron-devtools",
    "version": "0.0.3",
    "description": "All logs and network activity in one browser window",
    "main": "dist/testApp/index.js",
    "browserslist": [
        "last 1 chrome versions"
    ],
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "DevTools",
        "Debugging"
    ],
    "author": "Koneru Inc",
    "license": "CC0-1.0",
    "devDependencies": {
        "@babel/core": "^7.8.0",
        "@typescript-eslint/eslint-plugin": "^2.15.0",
        "@typescript-eslint/parser": "^2.15.0",
        "auto-changelog": "^1.16.2",
        "cache-loader": "^4.1.0",
        "cross-env": "^6.0.3",
        "css-loader": "^2.1.1",
        "electron": "^7.1.8",
        "electron-connect": "^0.6.3",
        "electron-is-dev": "^1.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-svelte3": "^2.7.3",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.0.7",
        "jest": "^24.9.0",
        "mini-css-extract-plugin": "^0.6.0",
        "node-sass": "^4.13.0",
        "np": "^5.2.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "q": "^1.5.1",
        "sass-loader": "^8.0.1",
        "serve": "^11.0.0",
        "style-loader": "^0.23.1",
        "svelte": "^3.0.0",
        "svelte-loader": "2.13.3",
        "svelte-preprocess": "^3.3.0",
        "svelte-preprocess-sass": "^0.2.0",
        "ts-loader": "^6.2.1",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.7.4",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.3.1"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production run-p build:main build:renderer",
        "build:main": "webpack --config=./webpack.main.config.js",
        "build:renderer": "webpack --config=./webpack.renderer.config.js",
        "dev": "run-p serve:main serve:renderer",
        "serve:main": "webpack-dev-server --config=./webpack.main.config.js",
        "serve:renderer": "webpack-dev-server --config=./webpack.renderer.config.js",
        "test": "eslint \"src/**/*js\" \"src/**/*ts\" --fix"
    },
    "dependencies": {
        "lodash": "^4.17.15",
        "loglevel": "^1.6.6",
        "svelte-spa-router": "^2.0.0",
        "uuid": "^3.3.3"
    }
}
